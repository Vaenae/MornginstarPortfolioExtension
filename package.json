{
  "name": "morningstarportfolioextension",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.2",
    "bulma": "0.9.2",
    "papaparse": "5.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.129",
    "@types/papaparse": "5.2.5",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "esbuild": "0.8.42",
    "esbuild-plugin-sass": "0.3.3",
    "eslint": "7.19.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "scripts": {
    "build": "node build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier \"{src,scripts}/**/*.{js,ts,jsx,tsx,css,json}\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "validate": "npm run prettier -- --list-different && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,scripts}**/*.{js,jsx,ts,tsx,css}": [
      "npm run format",
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vaenae/MorningstarPortfolioExtension.git"
  },
  "author": "Joonas Vanninen",
